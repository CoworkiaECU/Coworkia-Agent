{
  "name": "coworkia-agent",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/express-servidor/index.js",
    "start": "node src/express-servidor/index.js",
    "backup": "node scripts/backup-database.js",
    "backup:list": "node scripts/backup-database.js list",
    "cleanup": "node scripts/cleanup-expired-data.js",
    "cleanup:dry-run": "node scripts/cleanup-expired-data.js --dry-run",
    "migrate": "node scripts/migrate-to-sqlite.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.0",
    "googleapis": "^165.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "openai": "^6.7.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.4"
  }
}
